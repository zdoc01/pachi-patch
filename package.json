{
  "name": "pachi-pach",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "dev:db": "npx prisma studio",
    "dev:db:start": "docker run --name local-psql -v local_psql_data:/var/lib/postgresql/data -p 54320:5432 -e POSTGRES_PASSWORD=my_password -d postgres",
    "db:reset": "npx prisma db push --force-reset && npx prisma db seed",
    "export": "next export",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.4.0",
    "axios": "^1.1.2",
    "babel-plugin-superjson-next": "^0.4.4",
    "classnames": "^2.3.2",
    "next": "12.2.5",
    "next-auth": "^4.13.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "superjson": "^1.10.1",
    "swr": "^2.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/node": "24.10.11",
    "@types/react": "17.0.34",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "prettier": "2.8.3",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "4.4.4"
  }
}
